    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>Book</servlet-name>
    <servlet-class>Booking</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Book</servlet-name>
    <url-pattern>/book</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>tog</servlet-name>
    <servlet-class>Toggle</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>tog</servlet-name>
    <url-pattern>/toggle</url-pattern>
  </servlet-mapping>
